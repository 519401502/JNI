                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="123456"/>
            <TextView
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="123456"/>
            <TextView
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="123456"/>
        </com.example.aml.clearn.view.ViewGroupText>

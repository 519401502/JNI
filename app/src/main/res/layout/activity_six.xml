<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>
        <import type="com.example.aml.clearn.entity.Data" alias="Datatt"/>
        <variable
            name="data"
            type="Datatt"/>
